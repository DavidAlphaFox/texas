  %{texas_sqlite, ".*", {git, "https://github.com/emedia-project/texas_sqlite", "master"}},
  %{texas_mysql, ".*", {git, "https://github.com/emedia-project/texas_mysql", "master"}},
  %{texas_pgsql, ".*", {git, "https://github.com/emedia-project/texas_pgsql", "master"}}
