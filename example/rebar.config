  {texas_sqlite, ".*", {rsync, "../../texas_sqlite"}},
  {texas_mysql, ".*", {rsync, "../../texas_mysql"}},
  {texas_pgsql, ".*", {rsync, "../../texas_pgsql"}}
]}.

